<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".example.Exp2Activity">
    <Button
        android:id="@+id/btn_exp2_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="在渠道二发起一条通知，id为10"
        tools:ignore="HardcodedText"/>
    <Button
        android:id="@+id/btn_exp2_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="在渠道二更新一条通知，id为10"
        tools:ignore="HardcodedText"/>
    <Button
        android:id="@+id/btn_exp2_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="在渠道二发起一条通知，id为11\n带进度条，每次点击可以增加10%"
        tools:ignore="HardcodedText"/>
    <Button
        android:id="@+id/btn_exp2_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="在渠道三发起一条通知，id为12"
        tools:ignore="HardcodedText"/>
    <Button
        android:id="@+id/btn_exp2_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="进入系统通知设置"
        tools:ignore="HardcodedText"/>
</LinearLayout>