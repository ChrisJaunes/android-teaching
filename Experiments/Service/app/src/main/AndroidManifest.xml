<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chrisjaunes.android_teaching.service">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".example.ExpService"
            android:enabled="true"
            android:exported="true"></service>

        <activity android:name=".exp.MusicActivity" />
        <activity android:name=".exp.DownloadActivity" />
        <activity
            android:name=".example.Exp1Activity"
            android:label="实验一" />
        <activity
            android:name=".example.Exp2Activity"
            android:label="实验二" />
        <activity
            android:name=".example.Exp3Activity"
            android:label="实验三" />
        <activity
            android:name=".example.Exp4Activity"
            android:label="实验四" />
        <activity
            android:name=".example.Exp5Activity"
            android:label="实验五" />

        <service
            android:name=".example.Exp1Service"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name=".example.Exp2Service"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name=".example.Exp3Service"
            android:enabled="true"
            android:exported="false" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>